generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id     String   @id @unique @default(uuid())
  nome   String
  senha  String
  email  String
  pontos Pontos[]
}

model Pontos {
  id      Int    @id @default(autoincrement())
  usuario User   @relation(fields: [userId], references: [id])
  pontos  String
  userId  String
}
